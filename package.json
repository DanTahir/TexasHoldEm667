  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "esbuild backend/server.ts --bundle --platform=node --target=node20.12 --outfile=build/server.js",
    "start:dev": "concurrently \"npm run start:server:dev\" \"npm run build:dev:backend\" \"npm run build:dev:frontend\"",
    "build:dev:backend": "esbuild backend/server.ts --watch --bundle --platform=node --target=node20.12 --outfile=build/server.js",
    "build:dev:frontend": "esbuild frontend/index.ts --watch --bundle --outfile=backend/static/js/index.js",
    "start:server:dev": "NODE_ENV=development nodemon ./build/server.js",
    "migrate": "node-pg-migrate up -m ./migrations -j ts --tsconfig ./tsconfig.json",
    "migrate:create": "node-pg-migrate create -j=ts",
    "migrate:rollback": "node-pg-migrate down -m ./migrations -j ts --tsconfig ./tsconfig.json",
    "db:init": "./scripts/init-postgres.sh",
    "db:start": "./scripts/start-postgres.sh"
  },
